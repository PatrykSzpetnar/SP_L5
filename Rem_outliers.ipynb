      "source": [
        "def remove_outliers(group):\n",
        "    Q1 = group.quantile(0.25)\n",
        "    Q3 = group.quantile(0.75)\n",
        "    IQR = Q3 - Q1\n",
        "    lower_bound = Q1 - 1.5 * IQR\n",
        "    upper_bound = Q3 + 1.5 * IQR\n",
        "    return group[(group >= lower_bound) & (group <= upper_bound)]"
      ]
    }
  ]
}